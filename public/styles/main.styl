lightseagreen = #39bca4
cadetblue = #6c94b1
beidge = #fcfcf9
darkcyan = #0092b2
wheat =  #dfd3b6
body
    background beidge
    font-family 'Source Sans Pro', sans-serif
img
    width 100%
a
    color darkcyan

.no-float
    float none

.no-list
    margin 0
    padding 0
    text-align center
    li
        list-style-type none
        display inline-block
.avatar
    width 200px
    heigth 200px
    border-radius 50%

.inline-block
    display inline-block

.vertical-align
    vertical-align top

.margin-up
    margin-top 60px

.margin-right
    margin-right 1em

.pacifico
    font-family 'Pacifico', cursive

.big
    font-size 2.5em

.medium
    font-size 1.5em

.color-lightgreen
    color lightseagreen

.color-wheat
    background-color wheat

.padding-large
    padding 5px 10px

.padding-medium
    padding 5px

.color-blue
    color cadetblue
